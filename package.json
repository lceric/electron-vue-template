{
  "name": "electron-vue-template",
  "version": "0.1.0",
  "description": "A Vue-Electron application, built with vue-cli3ã€‚",
  "main": "electron/app.js",
  "homepage": "./",
  "scripts": {
    "dev": "vue-cli-service serve",
    "start:electron": "electron .",
    "lint": "vue-cli-service lint",
    "pack": "node electron/build.js",
    "build:vue": "vue-cli-service build",
    "build:electron": "electron-builder build --x64",
    "build:mac": "electron-builder build --x64 --mac",
    "build:win": "electron-builder build --win --x64",
    "commit": "git-cz",
    "precommit": "lint-staged"
  },
  "repository": "https://github.com/lichao-eric/electron-vue-template",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Eric Lee",
  "license": "CC0-1.0",
  "dependencies": {
    "core-js": "^3.4.3",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.0",
    "@vue/cli-plugin-vuex": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.0.3",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "2.1.0",
    "dart-sass": "^1.23.7",
    "electron": "^6.0.0",
    "electron-builder": "^21.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.19.1",
    "progress-bar-webpack-plugin": "^1.12.1",
    "sass-loader": "^8.0.0",
    "validate-commit-msg": "2.14.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.3",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "pre-commit": "precommit",
  "husky": {
    "hooks": {
      "commit-msg": "validate-commit-msg"
    }
  },
  "development": {
    "port": "7878"
  },
  "build": {
    "productName": "electron-vue-template",
    "appId": "com.electron.vue.template.app",
    "mac": {
      "target": "dmg",
      "icon": "electron/icon/electron-vue-template.png"
    },
    "win": {
      "target": "msi",
      "icon": "electron/icon/electron-vue-template.png"
    }
  }
}
