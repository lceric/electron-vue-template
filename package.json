{
  "name": "electron-vue-tpl",
  "version": "1.0.0",
  "description": "A Vue-Electron application, built with vue-cli3ã€‚",
  "main": "electron/app.js",
  "homepage": "./",
  "scripts": {
    "dev": "vue-cli-service serve",
    "start:electron": "electron .",
    "lint": "vue-cli-service lint",
    "pack": "node electron/build.js",
    "build:vue": "vue-cli-service build",
    "build:electron": "electron-builder build --x64",
    "build:mac": "electron-builder build --x64 --mac",
    "build:win": "electron-builder build --win --x64"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Eric Lee",
  "license": "CC0-1.0",
  "dependencies": {
    "core-js": "^3.4.3",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.0",
    "@vue/cli-plugin-vuex": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.0.3",
    "electron": "^6.0.0",
    "electron-builder": "^21.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "prettier": "^1.19.1",
    "progress-bar-webpack-plugin": "^1.12.1",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.10"
  },
  "build": {
    "productName": "electron-vue-tpl",
    "appId": "com.electron.vue.tpl.app",
    "mac": {
      "target": "dmg",
      "icon": "electron/icon/electron-vue-tpl.png"
    },
    "win": {
      "target": "msi",
      "icon": "electron/icon/electron-vue-tpl.png"
    }
  }
}
